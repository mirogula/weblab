#footer .copyright {
	text-align: center;
	margin-top: 5px;
}


.dp-xml {
    width:1000px;
}

.ui-themeswitcher a.ui-selectonemenu-label-container {
    text-decoration: none !important;
}

.ui-widget {
    font-size: 12px !important;
}


.no-border-grid tr, .no-border-grid td {
	border: none;
	border-style: hidden !important;
}


#menubar {
	
}

